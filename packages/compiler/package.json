{
  "name": "@vac-dsl/compiler",
  "version": "0.1.0",
  "description": "The vac-dsl compiler",
  "license": "MIT",
  "author": "Mick Dekkers",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "fmt": "yarn run -s prettier --write --list-different \"src/**/*.{ts,js}\"",
    "prepublish": "run-s build",
    "prettier": "prettier --single-quote --no-semi",
    "test": "jest"
  },
  "dependencies": {
    "@vac-dsl/core": "^0.1.0",
    "change-case": "^3.0.1",
    "crc-32": "^1.1.1",
    "dedent": "^0.7.0",
    "didyoumean2": "^1.3.0",
    "humanize-list": "^1.0.1",
    "is_js": "^0.9.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^21.1.6",
    "@types/node": "^8.0.49",
    "@types/ramda": "^0.25.3",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.1",
    "ts-jest": "^21.2.2",
    "typescript": "^2.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^@vac-dsl/(.*)": "<rootDir>/../$1/src"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@vac-dsl)"
    ],
    "mapCoverage": true
  },
  "private": false
}
