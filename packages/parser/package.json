{
  "name": "@vac-dsl/parser",
  "version": "0.1.0",
  "description": "The vac-dsl parser",
  "license": "MIT",
  "author": "Mick Dekkers",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "compile-grammar": "nearleyc src/grammar.ne | run-s -s prettier > src/grammar.ts",
    "fmt": "yarn run prettier --write \"src/**/*.{ts,js}\"",
    "prepublish": "run-s compile-grammar build",
    "prettier": "prettier --single-quote --no-semi"
  },
  "dependencies": {
    "@vac-dsl/core": "^0.1.0",
    "lodash": "^4.17.4",
    "nearley": "^2.11.0",
    "read-pkg-up": "^3.0.0"
  },
  "devDependencies": {
    "@types/nearley": "^2.11.0",
    "@types/node": "^8.0.49",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.1",
    "typescript": "^2.6.1"
  },
  "private": false
}
